project 'TheFashionist.xcodeproj'
 
# Uncomment the next line to define a global platform for your project
platform :ios, '14.0'
 
def shared_pods
pod 'Firebase/Core', '6.1.0'
pod 'Firebase/Auth', '6.1.0'
pod 'Firebase/Firestore', '6.1.0'
pod 'Firebase/Storage', '6.1.0'
pod 'Firebase/Functions', '6.1.0'
pod 'IQKeyboardManagerSwift', '6.3.0'
pod 'Kingfisher', '~> 4.0'
end
 
 
target 'TheFashionist' do
# Comment the next line if you're not using Swift and don't want to use dynamic frameworks
use_frameworks!
 
# Pods for TheFashionist
shared_pods
pod 'Stripe', '19.0.1'
end

target 'TheFashionistAdmin' do
# Comment the next line if you're not using Swift and don't want to use dynamic frameworks
use_frameworks!
 
# Pods for TheFashionistAdmin
shared_pods
end
